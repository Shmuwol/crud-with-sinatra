<h1>This is the lists page</h1>

<a href="/">Home</a>
<a href="/lists/new">Add A List</a>

<h3>My lists</h3>


<ul>
    <% @lists.each do |list| %>
        <li><a href="/lists/<%= list.id %>"><%= list.title %></a></li>
        <form method="post"
              action="/lists/<%= list.id %>/delete">
              <input id="hidden"
                     type="hidden"
                     name="_method"
                     value="DELETE">
              <input type="submit"
                     value="Delete List">
        </form>
    <% end %>
</ul>
